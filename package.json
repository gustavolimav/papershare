{
  "name": "papershare",
  "version": "1.0.0",
  "description": "[![Build](https://img.shields.io/badge/build-passing-brightgreen)]() [![Tests](https://img.shields.io/badge/tests-passing-brightgreen)]() [![License: MIT](https://img.shields.io/badge/license-MIT-blue.svg)](LICENSE) [![Contributions Welcome](https://img.shields.io/badge/contributions-welcome-orange.svg)]()",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && npm run services:wait:postgres && npm run migrations:up && next dev",
    "test": "npm run services:up && concurrently --names next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand\"",
    "test:name": "npm run test -- -t=",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "posttest": "npm run services:stop",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:wait:postgres": "tsx infra/scripts/wait-for-postgres.ts",
    "migrations:create": "echo 'Create migration files manually in infra/migrations/ with format: ###-description.sql'",
    "migrations:up": "tsx -e \"import dotenv from 'dotenv'; dotenv.config({ path: '.env.development' }); import migrator from './models/migrator'; migrator.runPendingMigrations().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "next lint --dir .",
    "prepare": "husky",
    "commit": "cz",
    "sf": "npm run lint:prettier:fix && npm run lint:eslint:check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gustavolimav/papershare.git"
  },
  "keywords": [],
  "author": "Gustavo Lima",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/gustavolimav/papershare/issues"
  },
  "homepage": "https://github.com/gustavolimav/papershare#readme",
  "dependencies": {
    "async-retry": "1.3.3",
    "bcryptjs": "3.0.2",
    "cookie": "1.0.2",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "next": "14.2.14",
    "next-connect": "1.0.0",
    "pg": "8.13.0",
    "postgres-migrations": "^5.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@commitlint/types": "^19.5.0",
    "@faker-js/faker": "^9.7.0",
    "@types/async-retry": "^1.4.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.25",
    "@types/set-cookie-parser": "^2.4.4",
    "@types/uuid": "^9.0.5",
    "commitizen": "4.3.1",
    "concurrently": "9.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-jsonc": "2.16.0",
    "eslint-plugin-no-secrets": "1.0.2",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "set-cookie-parser": "2.7.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0"
  }
}
